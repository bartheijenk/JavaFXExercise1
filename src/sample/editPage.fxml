<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<Scene xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller2">
   <root>
      <AnchorPane prefHeight="548.0" prefWidth="386.0">
         <children>
            <TextArea fx:id="directions" layoutX="93.0" layoutY="296.0" prefHeight="200.0" prefWidth="200.0" text="Directions" />
            <TextField fx:id="title" layoutX="91.0" layoutY="46.0" text="Title" />
            <ListView fx:id="ingredientListView" editable="true" layoutX="91.0" layoutY="96.0" prefHeight="200.0" prefWidth="200.0" />
            <Spinner fx:id="servings" layoutX="91.0" layoutY="71.0" />
            <Button fx:id="save" layoutX="165.0" layoutY="503.0" mnemonicParsing="false" onAction="#saveRecipe" text="Save" />
         </children>
      </AnchorPane>
   </root>
</Scene>
